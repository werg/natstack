{
  "type": "module",
  "name": "natstack",
  "version": "0.1.0",
  "private": true,
  "description": "A horizontally stacked panel app built with Electron and TypeScript",
  "main": "dist/main.cjs",
  "scripts": {
    "build": "NODE_ENV=production node build.mjs",
    "dev": "NODE_ENV=development node build.mjs && pnpm type-check & node scripts/run-electron.mjs --workspace=workspace",
    "dev:safe": "NODE_ENV=development node build.mjs && node scripts/run-electron.mjs",
    "start": "node scripts/run-electron.mjs",
    "cleanup": "bash scripts/cleanup-processes.sh",
    "lint": "eslint src --max-warnings 0",
    "format": "prettier --write 'src/**/*.{ts,tsx,css,html,json}'",
    "format:check": "prettier --check 'src/**/*.{ts,tsx,css,html,json}'",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch --preserveWatchOutput",
    "type-check:panels": "node scripts/type-check-panels.mjs",
    "type-check:all": "pnpm type-check && pnpm type-check:panels",
    "init:workspace": "node scripts/init-workspace.mjs"
  },
  "keywords": [
    "electron",
    "typescript",
    "panel"
  ],
  "author": "Gabriel Pickard",
  "license": "MIT",
  "packageManager": "pnpm@10.12.4",
  "pnpm": {
    "allowedBuilds": [
      "electron",
      "esbuild"
    ],
    "onlyBuiltDependencies": [
      "electron",
      "esbuild",
      "node-git-server"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@ryoppippi/unplugin-typia": "^2.6.5",
    "@types/node": "^24.10.1",
    "@types/npmcli__arborist": "^6.3.1",
    "@types/react": "^19.2.4",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "electron": "^39.1.2",
    "eslint": "^9.39.1",
    "prettier": "^3.6.2",
    "typescript-eslint": "^8.46.4"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.45",
    "@ai-sdk/google": "^2.0.43",
    "@ai-sdk/mistral": "^2.0.24",
    "@ai-sdk/openai": "3.0.0-beta.65",
    "@anthropic-ai/claude-agent-sdk": "^0.1.53",
    "@natstack/ai": "workspace:*",
    "@natstack/core": "workspace:*",
    "@natstack/panel": "workspace:*",
    "@natstack/react": "workspace:*",
    "@npmcli/arborist": "^7.4.0",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/themes": "^3.2.1",
    "@zenfs/core": "^2.4.4",
    "@zenfs/dom": "^1.2.5",
    "ai": "6.0.0-beta.114",
    "ai-sdk-provider-claude-code": "^2.2.1",
    "ai-sdk-provider-codex-cli": "^0.6.0",
    "dotenv": "^16.4.5",
    "esbuild": "^0.27.0",
    "jotai": "^2.15.1",
    "node-git-server": "^1.0.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "typescript": "^5.9.3",
    "typia": "^10.1.0",
    "yaml": "^2.8.1",
    "zod": "^4.1.12"
  }
}
