{
  "type": "module",
  "name": "natstack",
  "version": "0.1.0",
  "private": true,
  "description": "A horizontally stacked panel app built with Electron and TypeScript",
  "main": "dist/main.cjs",
  "scripts": {
    "postinstall": "electron-rebuild -f && bash commit-workspace.sh",
    "prebuild": "node scripts/generate-channels.mjs && node scripts/generate-injected.mjs",
    "build": "NODE_ENV=production node build.mjs",
    "dev": "NODE_ENV=development node build.mjs && (pnpm type-check & node scripts/run-electron.mjs --workspace=workspace)",
    "start": "node scripts/run-electron.mjs",
    "cleanup": "bash scripts/cleanup-processes.sh",
    "lint": "eslint src",
    "format": "prettier --write 'src/**/*.{ts,tsx,css,html,json}'",
    "format:check": "prettier --check 'src/**/*.{ts,tsx,css,html,json}'",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch --preserveWatchOutput",
    "pretest": "npm rebuild better-sqlite3",
    "test": "vitest run",
    "posttest": "electron-rebuild -f -w better-sqlite3 -o better-sqlite3",
    "test:watch": "vitest",
    "test:e2e": "playwright test -c tests/config/playwright.config.ts",
    "test:e2e:headed": "playwright test -c tests/config/playwright.config.ts --headed",
    "test:all": "pnpm test && pnpm test:e2e"
  },
  "keywords": [
    "electron",
    "typescript",
    "panel"
  ],
  "author": "Gabriel Pickard",
  "license": "MIT",
  "packageManager": "pnpm@10.12.4",
  "pnpm": {
    "allowedBuilds": [
      "electron",
      "esbuild",
      "better-sqlite3"
    ],
    "onlyBuiltDependencies": [
      "electron",
      "esbuild",
      "node-git-server",
      "better-sqlite3"
    ]
  },
  "devDependencies": {
    "@electron/rebuild": "^3.7.1",
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.57.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^24.10.1",
    "@types/npmcli__arborist": "^6.3.1",
    "@types/pacote": "^11.1.8",
    "@types/react": "^19.2.4",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "buffer": "^6.0.3",
    "electron": "^39.2.5",
    "eslint": "^9.39.1",
    "prettier": "^3.6.2",
    "typescript-eslint": "^8.46.4",
    "vitest": "^3.2.2"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.45",
    "@ai-sdk/google": "^2.0.43",
    "@ai-sdk/mistral": "^2.0.24",
    "@ai-sdk/openai": "3.0.0-beta.65",
    "@anthropic-ai/claude-agent-sdk": "^0.1.53",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@ghostery/adblocker": "^2.13.4",
    "@modelcontextprotocol/sdk": "^1.12.1",
    "@natstack/ai": "workspace:*",
    "@natstack/git": "workspace:*",
    "@natstack/git-ui": "workspace:*",
    "@natstack/react": "workspace:*",
    "@natstack/rpc": "workspace:*",
    "@natstack/runtime": "workspace:*",
    "@npmcli/arborist": "^7.4.0",
    "@openai/codex-sdk": "^0.73.0",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/themes": "^3.2.1",
    "@types/json-schema": "^7.0.15",
    "@types/ws": "^8.18.1",
    "@zenfs/core": "^2.4.4",
    "@zenfs/dom": "^1.2.5",
    "ai": "6.0.0-beta.114",
    "ai-sdk-provider-claude-code": "^2.2.1",
    "ai-sdk-provider-codex-cli": "^0.6.0",
    "ajv": "^8.17.1",
    "better-sqlite3": "^12.5.0",
    "chokidar": "^5.0.0",
    "cross-fetch": "^4.1.0",
    "dotenv": "^16.4.5",
    "esbuild": "^0.27.0",
    "jotai": "^2.15.1",
    "node-git-server": "^1.0.0",
    "p-limit": "^6.2.0",
    "pacote": "^21.0.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tldts": "^7.0.19",
    "typescript": "^5.9.3",
    "verdaccio": "^6.1.3",
    "ws": "^8.18.3",
    "yaml": "^2.8.1",
    "zod": "^3.24.1",
    "zod-to-json-schema": "^3.25.0"
  }
}
