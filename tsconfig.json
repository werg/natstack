{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "jsx": "react-jsx",
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "outDir": "./dist",
    "rootDir": ".",
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "noPropertyAccessFromIndexSignature": true,
    "allowUnusedLabels": false,
    "allowUnreachableCode": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "moduleResolution": "bundler",
    "types": ["node"],
    "baseUrl": ".",
    "paths": {
      "@natstack/types": ["packages/types/src/index.ts"],
      "@natstack/rpc": ["packages/rpc/src/index.ts"],
      "@natstack/git": ["packages/git/src/index.ts"],
      "@natstack/typecheck": ["packages/typecheck/src/index.ts"],
      "@workspace/core": ["workspace/packages/core/src/index.ts"],
      "@workspace/core/agent-types": ["workspace/packages/core/src/agent-types.ts"],
      "@workspace/core/ipc-protocol": ["workspace/packages/core/src/ipc-protocol.ts"],
      "@workspace/core/form-schema": ["workspace/packages/core/src/form-schema.ts"],
      "@workspace/core/config-types": ["workspace/packages/core/src/config-types.ts"],
      "@workspace/core/database": ["workspace/packages/core/src/database.ts"],
      "@workspace/core/base64": ["workspace/packages/core/src/base64.ts"],
      "@workspace/ai": ["workspace/packages/ai/src/index.ts"],
      "@workspace/pubsub": ["workspace/packages/pubsub/src/index.ts"],
      "@workspace/agentic-messaging": ["workspace/packages/agentic-messaging/src/index.ts"],
      "@workspace/agentic-messaging/config": ["workspace/packages/agentic-messaging/src/config-entry.ts"],
      "@workspace/agentic-messaging/session": ["workspace/packages/agentic-messaging/src/session-entry.ts"],
      "@workspace/agentic-messaging/tool-schemas": ["workspace/packages/agentic-messaging/src/tool-schemas-entry.ts"],
      "@workspace/agentic-messaging/utils": ["workspace/packages/agentic-messaging/src/utils-entry.ts"],
      "@workspace/agentic-messaging/client": ["workspace/packages/agentic-messaging/src/client-entry.ts"],
      "@workspace/agentic-messaging/image-utils": ["workspace/packages/agentic-messaging/src/image-utils-entry.ts"],
      "@workspace/agentic-messaging/protocol-schemas": ["workspace/packages/agentic-messaging/src/protocol-schemas-entry.ts"],
      "@workspace/agentic-messaging/async": ["workspace/packages/agentic-messaging/src/async-entry.ts"],
      "@workspace/agentic-messaging/json-schema": ["workspace/packages/agentic-messaging/src/json-schema-entry.ts"],
      "@workspace/agentic-messaging/recovery": ["workspace/packages/agentic-messaging/src/recovery-entry.ts"],
      "@workspace/react": ["workspace/packages/react/src/index.ts"],
      "@workspace/runtime": ["workspace/packages/runtime/src/panel/index.ts"],
      "@workspace/eval": ["workspace/packages/eval/src/index.ts"],
      "@workspace/agent-runtime": ["workspace/packages/agent-runtime/src/index.ts"],
      "@workspace/agent-patterns": ["workspace/packages/agent-patterns/src/index.ts"],
      "@workspace/agent-patterns/prompts": ["workspace/packages/agent-patterns/src/prompts/index.ts"],
      "@workspace/agent-patterns/queue": ["workspace/packages/agent-patterns/src/queue/index.ts"],
      "@workspace/agent-patterns/settings": ["workspace/packages/agent-patterns/src/settings/index.ts"],
      "@workspace/agent-patterns/trackers": ["workspace/packages/agent-patterns/src/trackers/index.ts"],
      "@workspace/agent-patterns/context": ["workspace/packages/agent-patterns/src/context/index.ts"],
      "@workspace/agent-patterns/interrupt": ["workspace/packages/agent-patterns/src/interrupt/index.ts"],
      "@workspace/agentic-chat": ["workspace/packages/agentic-chat/index.ts"],
      "@workspace/agentic-chat/types": ["workspace/packages/agentic-chat/types.ts"],
      "@workspace/agentic-tools": ["workspace/packages/agentic-tools/index.ts"],
      "@workspace/agentic-tools/types": ["workspace/packages/agentic-tools/types.ts"],
      "@workspace/agentic-components": ["workspace/packages/agentic-components/index.ts"],
      "@workspace/agentic-components/types": ["workspace/packages/agentic-components/types.ts"],
      "@workspace/context-template-editor": ["workspace/packages/context-template-editor/index.ts"],
      "@workspace/context-template-editor/types": ["workspace/packages/context-template-editor/types.ts"],
      "@workspace/about-shared/serviceAdapters": ["workspace/packages/about-shared/serviceAdapters.ts"],
      "@workspace/about-shared/types": ["workspace/packages/about-shared/types.ts"],
      "@workspace/tool-ui": ["workspace/packages/tool-ui/src/index.ts"],
      "@workspace/tool-ui/monaco": ["workspace/packages/tool-ui/src/components/tool-previews/monaco.ts"],
      "@workspace/git-ui": ["workspace/packages/git-ui/src/index.ts"],
      "@workspace/git-ui/constants": ["workspace/packages/git-ui/src/constants.ts"],
      "@workspace/git-ui/monaco": ["workspace/packages/git-ui/src/monaco.ts"],
      "@workspace-panels/project-panel/types": ["workspace/panels/project-panel/types.ts"],
      "modern-monaco/editor-core": ["workspace/packages/git-ui/node_modules/modern-monaco/types/monaco.d.ts"]
    }
  },
  "include": ["src/**/*", "packages/**/*", "workspace/**/*"],
  "exclude": ["node_modules", "dist", "**/dist", "workspace/packages/playwright-*"]
}
