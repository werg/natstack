{
  "manifest_version": 3,
  "name": "NatStack Panel Manager",
  "version": "0.1.0",
  "description": "Manages NatStack panel tabs â€” auto-opens panels, shows panel tree, clean tab management.",
  "permissions": [
    "tabs",
    "storage"
  ],
  "host_permissions": [
    "http://*.localhost/*",
    "http://127.0.0.1/*"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "NatStack Panels"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "icons": {}
}
