{
  "name": "@workspace/core",
  "version": "0.1.0",
  "description": "Minimal form-schema utility for workspace panels",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./form-schema": {
      "types": "./dist/form-schema.d.ts",
      "default": "./dist/form-schema.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "dev": "tsc --project tsconfig.build.json --watch"
  },
  "dependencies": {
    "@natstack/types": "workspace:*"
  },
  "devDependencies": {
    "typescript": "^5.9.3"
  }
}
