{
  "name": "@workspace-panels/chat-launcher",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "natstack": {
    "type": "app",
    "title": "New Chat",
    "entry": "index.tsx",
    "stateArgs": {
      "type": "object",
      "properties": {
        "channelName": {
          "type": "string",
          "description": "Existing channel to add agents to (channel modification mode)"
        }
      }
    },
    "exposeModules": [
      "react",
      "react/jsx-runtime",
      "react/jsx-dev-runtime",
      "@radix-ui/themes",
      "@radix-ui/react-icons"
    ]
  },
  "dependencies": {
    "@natstack/types": "workspace:*",
    "@workspace/runtime": "workspace:*",
    "@workspace/react": "workspace:*",
    "@workspace/agentic-messaging": "workspace:*",
    "@workspace/core": "workspace:*",
    "@natstack/pubsub": "workspace:*",
    "@radix-ui/themes": "^3.2.1",
    "@radix-ui/react-icons": "^1.3.2"
  }
}
