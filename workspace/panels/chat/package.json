{
  "name": "@workspace-panels/chat",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "natstack": {
    "type": "app",
    "title": "Agentic Chat",
    "entry": "index.tsx",
    "stateArgs": {
      "type": "object",
      "properties": {
        "channelName": {
          "type": "string",
          "description": "Channel to connect to"
        }
      },
      "required": ["channelName"]
    },
    "exposeModules": [
      "react",
      "react/jsx-runtime",
      "react/jsx-dev-runtime",
      "@radix-ui/themes",
      "@radix-ui/react-icons",
      "isomorphic-git"
    ]
  },
  "dependencies": {
    "@workspace/runtime": "workspace:*",
    "@workspace/react": "workspace:*",
    "@workspace/agentic-messaging": "workspace:*",
    "@workspace/agentic-chat": "workspace:*",
    "@workspace/agentic-tools": "workspace:*",
    "@radix-ui/themes": "^3.2.1",
    "@radix-ui/react-icons": "^1.3.2",
    "zod": "^3.22.0"
  }
}
