{
  "name": "@workspace-panels/project-panel",
  "version": "0.1.0",
  "type": "module",
  "exports": {
    ".": "./index.tsx",
    "./types": "./types.ts"
  },
  "natstack": {
    "type": "app",
    "title": "Project",
    "entry": "index.tsx",
    "stateArgs": {
      "type": "object",
      "required": ["projectConfig"],
      "properties": {
        "projectConfig": {
          "type": "object",
          "description": "Full project configuration"
        },
        "contextId": {
          "type": "string",
          "description": "Pre-created context ID for managed projects"
        }
      }
    },
    "exposeModules": [
      "react",
      "react/jsx-runtime",
      "react/jsx-dev-runtime",
      "@radix-ui/themes",
      "@radix-ui/react-icons"
    ]
  },
  "dependencies": {
    "@natstack/runtime": "workspace:*",
    "@natstack/react": "workspace:*",
    "@natstack/agentic-messaging": "workspace:*",
    "@workspace/context-template-editor": "workspace:*",
    "@radix-ui/themes": "^3.2.1",
    "@radix-ui/react-icons": "^1.3.2"
  }
}
