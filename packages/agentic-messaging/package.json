{
  "name": "@natstack/agentic-messaging",
  "version": "0.1.0-git.dfc926d",
  "description": "Agentic messaging protocol + SDK over @natstack/pubsub",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./config": {
      "types": "./dist/config-entry.d.ts",
      "default": "./dist/config-entry.js"
    },
    "./session": {
      "types": "./dist/session-entry.d.ts",
      "default": "./dist/session-entry.js"
    },
    "./recovery": {
      "types": "./dist/recovery-entry.d.ts",
      "default": "./dist/recovery-entry.js"
    }
  },
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "dev": "tsc --project tsconfig.build.json --watch",
    "test": "vitest run"
  },
  "dependencies": {
    "@natstack/core": "workspace*",
    "@natstack/pubsub": "workspace*",
    "ignore": "^5.3.2",
    "picomatch": "^4.0.0",
    "zod": "^3.24.1",
    "zod-to-json-schema": "^3.25.0"
  },
  "devDependencies": {
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "files": [
    "dist"
  ]
}
