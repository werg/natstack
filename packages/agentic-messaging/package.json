{
  "name": "@natstack/agentic-messaging",
  "version": "0.1.0-7b24c78153e0",
  "description": "Agentic messaging protocol + SDK over @natstack/pubsub",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./config": {
      "types": "./dist/config-entry.d.ts",
      "default": "./dist/config-entry.js"
    },
    "./session": {
      "types": "./dist/session-entry.d.ts",
      "default": "./dist/session-entry.js"
    },
    "./registry": {
      "types": "./dist/agent-registry.d.ts",
      "default": "./dist/agent-registry.js"
    },
    "./recovery": {
      "types": "./dist/recovery-entry.d.ts",
      "default": "./dist/recovery-entry.js"
    },
    "./tool-schemas": {
      "types": "./dist/tool-schemas-entry.d.ts",
      "default": "./dist/tool-schemas-entry.js"
    },
    "./utils": {
      "types": "./dist/utils-entry.d.ts",
      "default": "./dist/utils-entry.js"
    },
    "./client": {
      "types": "./dist/client-entry.d.ts",
      "default": "./dist/client-entry.js"
    },
    "./image-utils": {
      "types": "./dist/image-utils-entry.d.ts",
      "default": "./dist/image-utils-entry.js"
    },
    "./protocol-schemas": {
      "types": "./dist/protocol-schemas-entry.d.ts",
      "default": "./dist/protocol-schemas-entry.js"
    },
    "./async": {
      "types": "./dist/async-entry.d.ts",
      "default": "./dist/async-entry.js"
    },
    "./json-schema": {
      "types": "./dist/json-schema-entry.d.ts",
      "default": "./dist/json-schema-entry.js"
    }
  },
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "dev": "tsc --project tsconfig.build.json --watch",
    "test": "vitest run"
  },
  "dependencies": {
    "@natstack/core": "workspace:*",
    "@natstack/pubsub": "workspace:*",
    "ignore": "^5.3.2",
    "picomatch": "^4.0.0",
    "zod": "^3.24.1",
    "zod-to-json-schema": "^3.25.0"
  },
  "devDependencies": {
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "files": [
    "dist"
  ]
}
