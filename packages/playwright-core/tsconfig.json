{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "outDir": "./dist",
    "rootDir": "./src",
    "declaration": true,
    "declarationMap": true,
    "composite": true,
    "baseUrl": ".",
    "paths": {},
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "noPropertyAccessFromIndexSignature": false,
    "strict": false,
    "noImplicitAny": false
  },
  "include": ["src"],
  "exclude": ["types", "dist", "node_modules"]
}
