{
  "name": "@natstack/core",
  "version": "0.1.0",
  "description": "Core types shared between @natstack/runtime and @natstack/agent-runtime",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./agent-types": {
      "types": "./dist/agent-types.d.ts",
      "default": "./dist/agent-types.js"
    },
    "./ipc-protocol": {
      "types": "./dist/ipc-protocol.d.ts",
      "default": "./dist/ipc-protocol.js"
    },
    "./form-schema": {
      "types": "./dist/form-schema.d.ts",
      "default": "./dist/form-schema.js"
    },
    "./config-types": {
      "types": "./dist/config-types.d.ts",
      "default": "./dist/config-types.js"
    },
    "./database": {
      "types": "./dist/database.d.ts",
      "default": "./dist/database.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "dev": "tsc --project tsconfig.build.json --watch"
  },
  "devDependencies": {
    "typescript": "^5.9.3"
  }
}
