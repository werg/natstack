{
  "name": "@natstack/build-mdx",
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.js",
    "!dist/**/*.test.d.ts",
    "!dist/**/*.test.js.map",
    "!dist/**/*.test.d.ts.map"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "dev": "tsc --project tsconfig.build.json --watch",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@natstack/build": "workspace:*",
    "@mdx-js/mdx": "^3.1.1"
  },
  "peerDependencies": {
    "esbuild-wasm": "^0.24.0",
    "react": "^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "esbuild-wasm": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/react": "^19.1.0",
    "esbuild-wasm": "^0.24.0",
    "react": "^19.1.0",
    "typescript": "^5.9.3",
    "vitest": "^3.2.2"
  }
}
