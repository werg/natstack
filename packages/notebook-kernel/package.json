{
  "name": "@natstack/notebook-kernel",
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.js",
    "!dist/**/*.test.d.ts",
    "!dist/**/*.test.js.map",
    "!dist/**/*.test.d.ts.map"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "dev": "tsc --project tsconfig.build.json --watch",
    "test": "tsc --project tsconfig.json && node --test ./dist/*.test.js",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "acorn": "^8.14.0"
  },
  "peerDependencies": {
    "esbuild-wasm": "^0.24.0"
  },
  "peerDependenciesMeta": {
    "esbuild-wasm": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/acorn": "^4.0.6",
    "esbuild-wasm": "^0.24.0",
    "typescript": "^5.9.3"
  }
}
