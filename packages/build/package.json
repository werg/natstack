{
  "name": "@natstack/build",
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./browser": {
      "types": "./dist/browser.d.ts",
      "default": "./dist/browser.js"
    },
    "./config": {
      "types": "./dist/config.d.ts",
      "default": "./dist/config.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "dev": "tsc --project tsconfig.build.json --watch"
  },
  "dependencies": {
    "esbuild-wasm": "^0.25.5"
  },
  "peerDependencies": {
    "@zenfs/core": "^2.4.4"
  },
  "peerDependenciesMeta": {
    "@zenfs/core": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@zenfs/core": "^2.4.4",
    "typescript": "^5.9.3"
  }
}
